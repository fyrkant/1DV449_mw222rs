{
    "extends": "defaults/configurations/google",
    "rules": {
        "indent": [
            2,
            4
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars": 2,
        "no-invalid-this": 0,
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
}